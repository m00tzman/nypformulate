<%= simple_form_for @shipment do |f| %>

	<%= f.input :date %> <br>

	<%= f.input :day %> <br>

	<%= f.input :seal %> <br>

	<%= f.input :trucker %> <br>

	<%= f.input :truckernumber %> <br>

	<%= f.input :totalcases %> <br>

