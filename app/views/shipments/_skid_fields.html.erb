<div class="nested-fields">
	<div class = "col-md-12" id="fields">
		<div class = "col-md-2">
			<%= f.input :number, class: 'form-control' %>
		</div>

		<div class = "col-md-2">
			<%= f.input :po, class: 'form-control' %>
		</div>

		<div class = "col-md-2">
			<%= f.input :cases, class: 'form-control' %>
		</div>

		<div class = "col-md-4">
			<%= f.input :weight, class: 'form-control' %>
		</div>

		<div class = "col-md-2">
			<%= link_to_remove_association "remove", f %>
		</div>
		
	</div>
</div>
